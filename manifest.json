{
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "contentscript.js" ],
      "matches": [ "<all_urls>" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
   },
   "description": "Simple Drag & Drop Search",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png"
   },
   "manifest_version": 3,
   "name": "Simple Drag & Drop Search",
   "options_page": "options.html",
   "permissions": [ "tabs", "storage", "clipboardWrite", "clipboardRead", "downloads", "tts", "offscreen" ],
   "version": "1.0",
   "web_accessible_resources": [ {
      "resources": [ "l.png", "r.png", "u.png", "d.png", "lu.png", "ru.png", "ld.png", "rd.png", "s.png", "favicon/baidu.png", "favicon/bing.png", "favicon/duckduckgo.png", "favicon/flickr.png", "favicon/gmail.png", "favicon/google.png", "favicon/googlemaps.png", "favicon/imdb.png", "favicon/stackoverflow.png", "favicon/tumblr.png", "favicon/twitter.png", "favicon/wikipedia.png", "favicon/youtube.png", "favicon/amazon.png", "favicon/naver.png", "favicon/yandex.png", "favicon/yahoo.png", "favicon/hatena.png", "favicon/right.png", "favicon/clip.png", "favicon/vine.png", "favicon/pint.png", "favicon/reddit.png", "favicon/digg.png", "favicon/yanimg.png", "favicon/one.png", "favicon/az.png", "favicon/textfile.png", "favicon/memo.png", "favicon/speech.png" ],
      "matches": [ "<all_urls>" ]
   } ]
}
